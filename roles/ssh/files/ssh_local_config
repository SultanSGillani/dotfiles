Host 10.0.75.20
Hostname 10.0.75.20
user devops
IdentityFile ~/.ssh/id_ecdsa_devops

Host 10.0.75.102
Hostname 10.0.75.102
user devops
IdentityFile ~/.ssh/id_ecdsa_devops

Host 10.0.75.132
Hostname 10.0.75.132
user devops
IdentityFile ~/.ssh/id_ecdsa_devops

Host *
	# Always use SSH2.
	Protocol 2

	# Use a shared channel for all sessions to the same host,
	# instead of always opening a new one. This leads to much
	# quicker connection times.
    AddKeysToAgent yes
    #IdentityFile ~/.ssh/id_rsa
	# also this stuff
	Compression yes

